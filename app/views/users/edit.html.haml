.login-title
  %h2 Edit Account
  .account-page__content
    #account-page.account-page
      = form_for(current_user) do |f|
        .field
          .field-input
            = f.text_field :name, autofocus: true
        .field
          .field-input
            = f.email_field :email
        .actions
          = f.submit "Update", class: 'create-btn'